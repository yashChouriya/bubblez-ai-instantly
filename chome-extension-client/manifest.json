{
  "name": "Permian-AI",
  "version": "1.0",
  "description": "Permian Extension for instantly website.",
  "manifest_version": 3,
  "icons": {
    "16": "logo.png",
    "32": "logo.png"
  },
  "permissions": ["tabs", "activeTab", "scripting"],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content_script.v2.js"],
      "run_at": "document_idle",
      "all_frames": true
    }
  ]
}
